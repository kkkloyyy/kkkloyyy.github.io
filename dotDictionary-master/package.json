{
  "name": "dotdictionary",
  "version": "1.0.0",
  "description": "The only dictionary you will ever need",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --asar --icon ./src/assets/icon --out ./dist --overwrite",
    "build-darwin": "electron-packager . --platform darwin --asar --icon ./src/assets/icon.icns --out ./dist --overwrite",
    "build-linux": "electron-packager . --platform linux --asar --icon ./src/assets/icon.png --out ./dist --overwrite",
    "build-win32": "electron-packager . --platform win32 --asar --icon ./src/assets/icon.ico --out ./dist --overwrite",
    "build-all": "electron-packager . --all --asar --icon ./src/assets/icon --out ./dist --overwrite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Khaled Mohamed <Khal3d.Mohamed@gmail.com>",
  "contributors": [
    "Karim El-Sayed <Karim.Abdelghaffar98@gmail.com",
    "Rawan Ashraf <Rawan_Ashraf_98@hotmail.com>",
    "Mariam Kamel <MariamKamel98@hotmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "electron": "^1.7.9",
    "google-translate-api": "^2.3.0",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "electron-packager": "^10.1.0"
  }
}
